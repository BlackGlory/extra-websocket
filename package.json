{
  "name": "extra-websocket",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-websocket.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "build": "run-p build:*",
    "build:es2015": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2018": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/jest": "^27.5.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "husky": "4",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.3",
    "ts-patch": "^2.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "typescript-transform-paths": "^3.3.1",
    "ws": "^8.7.0"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.1",
    "@blackglory/structures": "^0.5.1",
    "@blackglory/wait-for": "^0.4.4",
    "extra-abort": "^0.1.2",
    "extra-timers": "^0.2.5"
  },
  "peerDependencies": {
    "ws": "^8.7.0"
  }
}
